{
  "format_version": "1.21.0",
  "minecraft:feature_rules": {
    "description": {
      "identifier": "bioscape:caldera_magma_feature_rule",
      "places_feature": "bioscape:gravel_canyon/underwater_magma_feature"
    },
    "conditions": {
      "placement_pass": "after_surface_pass",
      "minecraft:biome_filter": [
        {
          "test": "has_biome_tag",
          "operator": "==",
          "value": "gravel_canyon"
        }
      ]
    },
    "distribution": {
      "iterations": 3,
      "x": {
        "distribution": "uniform",
        "extent": [ 0, 16 ]
      },
      "y": "(q.heightmap(v.worldx, v.worldz) >= 77 && (q.noise(v.worldx/32, v.worldz/32) + q.noise(v.worldx/64, v.worldz/64) + q.noise(v.worldx/128, v.worldz/128) + q.noise(v.worldx/8, v.worldz/8)*0.1) > 0.4) ? q.heightmap(v.worldx, v.worldz) -2 : -1000",
      "z": {
        "distribution": "uniform",
        "extent": [ 0, 16 ]
      }
    }
  }
}
