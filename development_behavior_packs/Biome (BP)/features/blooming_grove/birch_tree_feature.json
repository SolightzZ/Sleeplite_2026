{
  "format_version": "1.21.0",
  "minecraft:tree_feature": {
    "description": {
      "identifier": "bioscape:blooming_grove/birch_tree_feature"
    },
    "acacia_trunk": {
      "trunk_width": 1,
      "trunk_height": {
        "base": 5,
        "intervals": [ 3, 3 ]
      },
      "trunk_block": {
        "name": "minecraft:log",
        "states": {
          "old_log_type": "birch"
        }
      },
      "trunk_lean": {
        "allow_diagonal_growth": true,
        "lean_height": {
          "range_min": 1,
          "range_max": 3
        },
        "lean_steps": {
          "range_min": 4,
          "range_max": 8
        }
      },
      "branches": {
        "branch_chance": 100.0,
        "branch_length": {
          "range_min": 2,
          "range_max": 3
        },
        "branch_position": {
          "range_min": 1,
          "range_max": 3
        },
        "branch_canopy": {
          "fancy_canopy": {
            "height": 3,
            "radius": 3,
            "leaf_block": {
              "name": "minecraft:leaves",
              "states": {
                "old_leaf_type": "birch"
              }
            }
          }
        }
      }
    },
    "fancy_canopy": {
      "height": 3,
      "radius": 3,
      "leaf_block": {
        "name": "minecraft:leaves",
        "states": {
          "old_leaf_type": "birch"
        }
      }
    },
    "may_grow_on": [
      { "tags": "query.any_tag('dirt')" }
    ],
    "base_block": [
      "minecraft:dirt",
      "minecraft:coarse_dirt"
    ],
    "may_grow_through": [
      { "tags": "query.any_tag('dirt')" }
    ],
    "may_replace": [
      "minecraft:air",
      "minecraft:snow_layer",
      "minecraft:acacia_leaves",
      "minecraft:azalea_leaves",
      "minecraft:azalea_leaves_flowered",
      "minecraft:birch_leaves",
      "minecraft:cherry_leaves",
      "minecraft:dark_oak_leaves",
      "minecraft:jungle_leaves",
      "minecraft:mangrove_leaves",
      "minecraft:oak_leaves",
      "minecraft:pale_oak_leaves",
      "minecraft:spruce_leaves"
    ]
  }
}
